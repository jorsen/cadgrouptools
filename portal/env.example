# Database Configuration
DATABASE_URL=mongodb+srv://username:password@cluster.mongodb.net/cadgroup?retryWrites=true&w=majority

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-here

# Storage Configuration - Supabase Storage (recommended)
STORAGE_DRIVER=supabase                # 'supabase' only

# Supabase Configuration
SUPABASE_URL=your-supabase-project-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE=your-supabase-service-role-key
SUPABASE_BUCKET=cadgroupmgt

# Email Configuration - SendGrid (Required for password reset emails)
SENDGRID_API_KEY=your-sendgrid-api-key
SENDGRID_FROM_EMAIL=notifications@cadgroupmgt.com
SENDGRID_FROM_NAME=CADGroup Tools Portal

# Legacy Email Configuration (Optional - for NextAuth)
EMAIL_FROM=noreply@cadgrouptools.com
EMAIL_SERVER_HOST=smtp.sendgrid.net
EMAIL_SERVER_PORT=587
EMAIL_SERVER_USER=apikey
EMAIL_SERVER_PASSWORD=your-sendgrid-api-key

# OCR Configuration - DEPRECATED (Now handled by Manus AI)
# Tesseract.js is used for all OCR processing (no API keys required)
# OCR works automatically without any configuration

# Manus AI Configuration
MANUS_API_KEY=your-manus-api-key
MANUS_BASE_URL=https://api.manus.ai/v1
MANUS_WEBHOOK_SECRET=your-generated-webhook-secret

# GoHighLevel Configuration
GHL_API_KEY=your-ghl-api-key
GHL_LOCATION_ID=your-ghl-location-id
GHL_WEBHOOK_SECRET=your-generated-ghl-webhook-secret

# Claude SDK Configuration
ANTHROPIC_API_KEY=your-anthropic-api-key

# Company Configuration
MURPHY_HOURLY_RATE=35

# Other Configuration
NODE_ENV=development
